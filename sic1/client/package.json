{
  "name": "sic1-client",
  "version": "1.11.0",
  "description": "Single-instruction computer programming game (web client)",
  "scripts": {
    "intl:extract": "formatjs extract \"ts/*.ts*\" --ignore=\"*/*.d.ts\" --out-file content/en/messages.json --preserve-whitespace --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "intl:pseudoloc": "formatjs extract \"ts/*.ts*\" --ignore=\"*/*.d.ts\" --out-file content/en/messages.json --preserve-whitespace && formatjs compile content/en/messages.json --ast --pseudo-locale en-XA --out-file content/en/compiled-messages.json",
    "test": "pushd test && mocha --require ts-node/register *.spec.ts && popd",
    "watch:test": "npx mocha --watch --require ts-node/register test/*.spec.ts --watch --watch-extensions ts",
    "build": "parcel build --no-cache --public-url ./ index.html",
    "build:dev": "parcel build --no-cache --no-optimize --public-url ./ index.html",
    "build:intl": "formatjs compile content/en/messages.json --ast --out-file content/en/compiled-messages.json",
    "build:mail": "ts-node build/build-mail.ts",
    "serve": "parcel serve --no-cache index.html"
  },
  "source": "index.html",
  "devDependencies": {
    "@formatjs/cli": "^6.2.4",
    "@types/lz-string": "^1.3.34",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.0",
    "md2jsx": "^1.1.0",
    "mocha": "^10.2.0",
    "parcel": "^2.11.0",
    "parcel-namer-hashless": "^1.0.3",
    "process": "^0.11.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "backtrace-js": "^1.1.3",
    "crs_queue": "^3.2.1",
    "lz-string": "^1.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.5.5",
    "react16": "^1.0.0",
    "sic1-server-contract": "../server/contract/dist"
  }
}
